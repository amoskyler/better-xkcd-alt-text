{
  "manifest_version": 2,
  "name": "Better XKCD Alt Text",
  "description": "This extension diplays the XKCD alt text after clicking a panel.",
  "version": "0.1",
  "icons": {
  },
  "content_scripts": [
    {
      "matches": [
        "http://xkcd.com/*",
        "https://xkcd.com/*"
      ],
      "js": ["insertPreview.js"],
      "css": ["css/main.css"]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
